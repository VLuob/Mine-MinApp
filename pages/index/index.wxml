<!--index.wxml-->
<swiper id="h" indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}">
  <block wx:for="{{background}}" wx:key="*this">
    <navigator url="/pages/logs/logs?a=1&b=2">
      <swiper-item>
        <view class="swiper-item {{item}}"></view>
      </swiper-item>
    </navigator>
  </block>
</swiper>
